#
# Ansible managed
#
exclude_paths:
  - ./meta/exception.yml
  - ./meta/preferences.yml
  - ./molecule/default/verify.yml
  - ./meta/main.yml

skip_list:
  - skip_ansible_lint
  - '106'
  - '208'
  - '702'
  - fqcn-builtins

warn_list:  # or 'skip_list' to silence them completely
  - internal-error  # Unexpected internal error.
